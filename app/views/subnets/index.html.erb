<p id="notice"><%= notice %></p>

<h1>Listing Subnets</h1>

<table>
  <thead>
    <tr>
      <th>Subnet addr</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @subnets.each do |subnet| %>
      <tr>
        <td><%= subnet.subnet_addr %></td>
        <td><%= subnet.description %></td>
        <td><%= link_to 'Show', subnet %></td>
        <td><%= link_to 'Edit', edit_subnet_path(subnet) %></td>
        <td><%= link_to 'Destroy', subnet, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Subnet', new_subnet_path %>
